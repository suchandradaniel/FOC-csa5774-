#include <stdio.h>

void generateSeries(int n) {
    int i;
    float num = 1.2;
    
    printf("Series: 1.2");
    for (i = 2; i <= n; i++) {
        printf(", %.1f", num + i - 1);
    }
    
    printf("\n");
}

int main() {
    int n;

    printf("Enter the value of n: ");
    scanf("%d", &n);

    generateSeries(n);

    return 0;
}
